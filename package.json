{
  "name": "txt-hours-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-grammar": "ohm generateBundles --esm --withTypes src/syntax/grammar/txt-hours.ohm",
    "build": "npm run build-grammar && tsc -b && vite build",
    "format": "prettier -w .",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "ohm-js": "^17.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "temporal-polyfill": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@ohm-js/cli": "^2.0.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-dom": "^1.52.9",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "^1.52.9",
    "globals": "^16.4.0",
    "prettier": "^3.7.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  }
}
